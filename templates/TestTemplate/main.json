{
  "name": "TestTemplate",
  "description": "Example for documentation",
  "path": "/templates/TestTemplate",
  "param": [
    "name"
  ],
  "selectParam": [
    {
      "paramName": "ide",
      "paramValue": [
        "AndroidStudio",
        "Pycharm",
        "intellij"
      ]
    },
    {
      "paramName": "version",
      "paramValue": [
        "3.3",
        "3.4",
        "3.5",
        "3.6"
      ]
    }
  ],
  "addFile": [
    {
      "name": "{name}[-C]ViewModel.kt",
      "path": "viewModels/{name}[-c]",
      "fileTemplatePath": "example.tm"
    },
    {
      "name": "{name}[-C]ViewModelTest.kt",
      "path": "test/viewModels/{name}[-c]",
      "fileTemplatePath": "example.tm"
    },
    {
      "name": "{name}[-C].txt",
      "path": "~",
      "fileTemplatePath": "example.tm"
    },
    {
      "name": "",
      "path": "path/test/catalog",
      "fileTemplatePath": "example.tm"
    },
    {
      "name": "{name}[-s]_main.xml",
      "path": "res/layout",
      "fileTemplatePath": "layout.tm"
    }
  ]
}